{
  "name": "gmose-penta",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:stg": "tsc && vite build --mode staging",
    "preview": "vite preview",
